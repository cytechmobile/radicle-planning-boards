{
  "git.alwaysSignOff": true,
  "conventionalCommits.scopes": [
    "repo",
    "dev",
    "build",
    "lint",
    "ci",
    "git",
    "deps",
    "changelog",
    "readme",
    "contributing",
    "package",
    "app"
  ],
  "conventionalCommits.gitmoji": false,

  // tailwindcss
  "css.validate": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },


}